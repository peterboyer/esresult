{
	"name": "esenum",
	"version": "1.0.0",
	"license": "MIT",
	"author": "Peter Boyer <https://github.com/peterboyer>",
	"repository": "github:peterboyer/esenum",
	"description": "0kb, Rust-like Enums for TypeScript.",
	"scripts": {
		"dev": "yarn test && yarn lint && yarn lint:ts",
		"test": "./scripts/test.sh",
		"test:e2e": "./scripts/test-e2e.sh",
		"lint": "./scripts/lint.sh",
		"lint:ts": "./scripts/lint-ts.sh",
		"build": "./scripts/build.sh",
		"docs": "./scripts/docs.sh"
	},
	"types": "src/index.d.ts",
	"exports": {
		".": {
			"types": "src/index.ts"
		},
		"./enum": {
			"types": "src/enum.global.entry.d.ts"
		},
		"./result": {
			"types": "src/result.global.entry.d.ts"
		},
		"./future": {
			"types": "src/future.global.entry.d.ts"
		},
		"./fns/match": {
			"main": "src/fns/match.d.ts",
			"types": "src/fns/match.d.ts"
		},
		"./fns/safely": {
			"main": "src/fns/safely.js",
			"types": "src/fns/safely.d.ts"
		}
	},
	"devDependencies": {
		"@tsconfig/node-lts-strictest": "^18.12.1",
		"@types/jest": "^29.4.1",
		"@typescript-eslint/eslint-plugin": "^5.55.0",
		"@typescript-eslint/parser": "^5.55.0",
		"concurrently": "^7.6.0",
		"dot-json": "^1.2.2",
		"esbuild": "^0.17.11",
		"esbuild-jest": "^0.5.0",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-plugin-prettier": "^4.2.1",
		"jest": "^29.5.0",
		"prettier": "^2.8.4",
		"tsd": "^0.27.0",
		"typedoc": "^0.23.26",
		"typescript": "^4.9.5"
	},
	"keywords": [
		"es",
		"enum",
		"result",
		"future",
		"match",
		"ok",
		"error",
		"async",
		"promise",
		"monad",
		"fp",
		"functional",
		"typescript"
	]
}
